encoding  utf-8

# Name of the source file.
source "tests/map_plan.th"

layout lgd
  origin 20 25 0 m
  scale 1 110
  base-scale 1 200
  color preview-below [50 50 60]
  color preview-above [60 50 50]
  symbol-set SKBB
  #symbol-hide group all
  #symbol-assign special north-arrow SKBB
  statistics explo off
  language sk
endlayout

# Export settings.
export map -layout lgd -layout-doc-title "Pokusna jaskyna" -layout-layers off
export database -encoding ASCII
