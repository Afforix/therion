encoding  utf-8
source rabbit.th

layout water-blue
  code metapost
  def a_water (expr p) =
    T:=identity;
    thfill p withcolor (0.0, 0.5, 1.0);
  enddef;
endlayout

export map -projection plan -output rabbit-plan.pdf -layout-map-header 0 0 s
export map -projection extended -output rabbit-xelev.pdf -layout-map-header 0 110 sw -layout water-blue
export model -output rabbit.lox
